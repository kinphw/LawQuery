{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug LawQuery Backend (TS)",
        "runtimeExecutable": "npx",
        "runtimeArgs": [
          "ts-node",
          "src/backend/ts/index.ts"
        ],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "skipFiles": ["<node_internals>/**"],
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  